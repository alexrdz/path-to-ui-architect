---
import { getCollection } from 'astro:content';
import MainLayout from '../../layouts/MainLayout.astro';

const posts = await getCollection('testing');
---

<MainLayout title="Testing">
  <h1>Testing</h1>
  <ul>
    {posts.map(post => (
      <li>
        <a href={`/testing/${post.slug}`}>
          {post.data.title}
        </a>
      </li>
    ))}
  </ul>
</MainLayout>
