---
import { getCollection } from 'astro:content';
import MainLayout from '../../layouts/MainLayout.astro';

const posts = await getCollection('architecture');
---

<MainLayout title="Architecture">
  <h1>Architecture</h1>
  <ul>
    {posts.map(post => (
      <li>
        <a href={`/architecture/${post.slug}`}>
          {post.data.title}
        </a>
      </li>
    ))}
  </ul>
</MainLayout>
