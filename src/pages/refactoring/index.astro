---
import { getCollection } from 'astro:content';
import MainLayout from '../../layouts/MainLayout.astro';
import ArticleList from '../../components/ArticleList.astro';

const posts = await getCollection('refactoring');
---

<MainLayout title="Refactoring">
  <h1>Refactoring</h1>
  <ArticleList posts={posts} collectionName="refactoring" />
</MainLayout>
